{
  "name": "pdf-processor",
  "description": "PDF manipulation and extraction toolkit for processing PDF files, extracting text, tables, and creating new PDFs",
  "category": "document-processing",
  "tags": ["pdf", "document", "extraction", "processing"],
  "version": "1.0.0",
  "author": "Claude Code",
  "configuration": {
    "required_env": [],
    "optional_env": [],
    "parameters": {
      "output_dir": {
        "type": "string",
        "description": "Directory for output files",
        "default": "./output"
      },
      "max_file_size": {
        "type": "number",
        "description": "Maximum PDF file size in MB",
        "default": 100
      }
    }
  },
  "capabilities": [
    "Extract text from PDF files",
    "Extract tables from PDF files",
    "Create new PDF documents",
    "Merge multiple PDFs",
    "Split PDF documents",
    "Fill PDF forms",
    "Convert PDF to images"
  ],
  "usage_examples": [
    {
      "description": "Extract text from a PDF file",
      "command": "extract_text input.pdf --output text.txt"
    },
    {
      "description": "Create a new PDF with merged content",
      "command": "merge input1.pdf input2.pdf --output merged.pdf"
    }
  ],
  "installation": {
    "npm": "npm install -g pdf-processor",
    "pip": "pip install pdf-processor"
  },
  "documentation_url": "https://github.com/your-repo/pdf-processor"
}